<section id="stationary" class="pt-3 pt-md-4 bg-secondary py-3">
  <h2 class="h3 text-center">Stationary</h2>
  <div class="tns-carousel tns-controls-static tns-controls-outside tns-nav-enabled pt-2">
    <div class="tns-carousel-inner" id="tns2"
      data-carousel-options='{"items": 2, "gutter": 32, "controls": true, "autoHeight": true, "responsive": {"0":{"items":1}, "480":{"items":2}, "720":{"items":3}, "991":{"items":2}, "1140":{"items":3}, "1300":{"items":4}, "1500":{"items":4}}}'>
      <% products.forEach(function(product){ %>
         <!--single product-->
         <div class="col-sm-6 col-md-4 col-lg-6 col-xl-3">
          <div class="card product-card card-static py-2 px-3">
            <span class="badge bg-danger badge-shadow">Sale</span>
            <div class="col-6 image-container mx-auto">
              <a class="card-img-top d-block overflow-hidden" href="grocery-single.html">
                <img
                  src=<%= product.imageUrl %>
                  alt="Product" class="loaded tns-complete">
              </a>
            </div>
            <div class="card-body py-2"><a class="product-meta d-block fs-xs pb-1" href="#">Fruits and Vegetables</a>
              <h3 class="product-title fs-sm text-truncate"><a href="grocery-single.html">Coconut, Indonesia (piece)</a>
              </h3>
              <div class="product-price">
                <span class="text-accent">Rs.<%= product.origPrice %><small>.00</small></span>
                <del class="fs-sm text-muted">$2.<small>99</small></del>
              </div>
            </div>
            <div class="product-floating-btn">
              <button class="btn btn-primary btn-shadow btn-sm" type="button">+<i
                  class="ci-cart fs-base ms-1"></i></button>
            </div>
  
          </div>
        </div>
      <%}); %>
    </div>
    </div>  
  </div>

  </div>
</section>
<% } %>

