<%- include('navbar'); -%>
<div class="page-title-overlap bg-dark pt-4 mt-5">
    <div class="container d-lg-flex justify-content-between py-lg-3">
        <div class="order-lg-1 pe-lg-4 text-center text-lg-start pt-1">
            <h1 class="h3 text-light mb-0">Seller Onboarding</h1>
        </div>
    </div>
</div>
<div class="container">
    <div class="row mb-0">
      <section class="col-lg-8">
        <!-- Steps-->
        <!-- Steps-->
        <div class="steps steps-light pt-2 pb-3 mb-5">
          <a class="step-item active">
            <div class="step-progress"><span class="step-count">1</span></div>
            <div class="step-label"><i class="ci-user-circle"></i>Signup</div>
          </a>
          <a class="step-item active" href="/selleraddress">
            <div class="step-progress"><span class="step-count">2</span></div>
            <div class="step-label"><i class="ci-store"></i>Details</div>
          </a>
          <a class="step-item active" href="/sellergst">
            <div class="step-progress"><span class="step-count">3</span></div>
            <div class="step-label"><i class="ci-calculator"></i>GST</div>
          </a>
          <a class="step-item active current" href="/sellerbank">
            <div class="step-progress"><span class="step-count">4</span></div>
            <div class="step-label"><i class="ci-wallet"></i>Bank Details</div>
          </a>
          <a class="step-item" href="/complete">
            <div class="step-progress"><span class="step-count">5</span></div>
            <div class="step-label"><i class="ci-card"></i>Complete</div>
          </a>
        </div>
      </section>
    </div>
</div>  
<% if (message){%>
  <div class="container alert alert-danger d-flex message-div" role="alert">
    <div class="alert-icon">
      <i class="ci-close-circle"></i>
    </div>
    <div class="message"><%= message %></div>
  </div>  

<%}%>
<!--main body-->
<div class="container pb-3 mb-5">
  <div class="bg-light shadow-lg rounded-3 overflow-hidden">
    <form class="px-3" action="/verifysellerbank" method="POST">
      <input type="hidden" name="_csrf" value="<%= csrfToken %>">
      <h2 class="h3 py-1 me-2 text-center text-sm-start">Enter Bank Details</h2>
      <div class="row">
        <div class="col-sm-4">
          <div class="">
            <label class="form-label" for="checkout-fn">Account Holder</label>
            <input class="form-control" type="text" id="checkout-fn" name="acctname" value="<%= acctname %>" disabled>
          </div>
        </div>
        <div class="col-sm-4">
          <div class="">
            <label class="form-label" for="checkout-fn">Account Number</label>
            <input class="form-control" type="text" id="checkout-fn" name="acctnum" value="<%= acctnum %>" disabled>
            
          </div>
        </div>
        <div class="col-sm-4">
          <div class="">
            <label class="form-label" for="checkout-phone">IFSC Code</label>
            <input class="form-control" type="text" id="checkout-phone" name="ifsc" value="<%= ifsc %>" disabled>
          </div>
        </div>
        <div class="form-text mb-3">On clicking pay,You will be redirected to payment protal to make payment of Rs.1 from entered bank account number.</div>
      </div>  
      <div class="row mt-2 d-none">
        <div class="col-sm-4">
          <div class="mb-3">
            <button class="btn btn-primary bankBtn w-100" id="razor-pay1" type="submit">
              <span class="d-none d-sm-inline">Save</span><span class="d-inline d-sm-none">Save</span>
            </button>
          </div>
        </div>
      </div>
      <input type="hidden" id="razorpay_payment_id" name="razorpay_payment_id" value="">
      <input type="hidden" id="razorpay_order_id" name="razorpay_order_id" value="">
      <input type="hidden" id="razorpay_signature" name="razorpay_signature" value="">
      <input type="hidden" id="error_code" name="razorpay_err_code" value="">
      <input type="hidden" id="error_desc" name="razorpay_err_desc" value="">
      <input type="hidden" id="error_source" name="razorpay_err_source" value="">
      <input type="hidden" id="error_reason" name="razorpay_err_reason" value="">
      <input class="form-control" type="hidden" id="checkout-fn" name="acctid" value="<%= acctid %>">

    </form>
    <!-- desktop navigation-->
    <div class="row d-none d-lg-flex mx-2 mb-2">
      <div class="col-lg-8 mb-2">
        <div class="d-none d-lg-flex pt-1 mt-1">
          <div class="w-50 pe-3">
            <a class="btn btn-secondary d-block w-100" href="/sellerbank">
